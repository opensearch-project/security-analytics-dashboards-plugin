{
  "title": "Suspicious Characters in CommandLine",
  "id": "2c0d2d7b-30d6-4d14-9751-7b9113042ab9",
  "status": "experimental",
  "description": "Detects suspicious Unicode characters in the command line, which could be a sign of obfuscation or defense evasion",
  "date": "2022/04/27",
  "author": "Florian Roth",
  "references": ["https://www.wietzebeukema.nl/blog/windows-command-line-obfuscation"],
  "logsource": {
    "product": "windows",
    "category": "process_creation"
  },
  "detection": {
    "selection_spacing_modifiers": {
      "CommandLine|contains": ["ˣ", "˪", "ˢ"]
    },
    "selection_unicode_slashes": {
      "CommandLine|contains": ["∕", "⁄"]
    },
    "selection_unicode_hyphens": {
      "CommandLine|contains": ["―", "—"]
    },
    "condition": "1 of selection*"
  },
  "falsepositives": ["Unknown"],
  "level": "high"
}
