{
  "title": "Advanced IP/Port Scanner Update Check",
  "id": "1a9bb21a-1bb5-42d7-aa05-3219c7c8f47d",
  "status": "experimental",
  "description": "Detect update check performed by Advanced IP Scanner and Advanced Port Scanner",
  "author": "Axel Olsson",
  "references": ["https://www.advanced-ip-scanner.com/", "https://www.advanced-port-scanner.com/"],
  "tags": ["attack.discovery", "attack.t1590"],
  "date": "2022/08/14",
  "logsource": {
    "category": "proxy"
  },
  "detection": {
    "selection": {
      "c-uri|contains": "/checkupdate.php",
      "c-uri-query|contains|all": ["lng=", "ver=", "beta=", "type=", "rmode=", "product="]
    },
    "condition": "selection"
  },
  "fields": ["c-ip"],
  "falsepositives": ["Legitimate use by administrators"],
  "level": "medium"
}
